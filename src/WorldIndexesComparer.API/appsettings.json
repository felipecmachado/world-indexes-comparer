{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Clients": {

    "Covid19": {
      "BaseUri": "https://api.covid19api.com",
      "MaxRetryAttempts": 2,
      "MaxConsecutiveFailures": 5,
      "MaxCircuitBreakerWaitingTime": 15,
      "Timeout": "00:00:30"
    },

    "RestCountries": {
      "BaseUri": "https://restcountries.com/v3.1",
      "MaxRetryAttempts": 2,
      "MaxConsecutiveFailures": 5,
      "MaxCircuitBreakerWaitingTime": 15,
      "Timeout": "00:00:30"
    }
  },

  "AllowedHosts": "*"
}
